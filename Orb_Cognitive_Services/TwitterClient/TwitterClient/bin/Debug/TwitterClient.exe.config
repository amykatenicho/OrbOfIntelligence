<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <appSettings>
    <!-- Twitter Streaming API settings.  Replace Twitter OAuth settings with your own -->
    <add key="oauth_consumer_key" value="<ENTER YOUR TWITTER CONSUMER KEY>" />
    <add key="oauth_consumer_secret" value="<ENTER YOUR TWITTER CONSUMER SECRET>" />
    <add key="oauth_token" value="<ENTER YOUR TWITTER TOKEN>" />
    <add key="oauth_token_secret" value="<ENTER YOUR TWITTER TOKEN SECRET>" />
    
    <!-- Cognitive Service, Text Analytics API Service-->
    <add key="accountkey" value="<COGNITIVE SERVICE KEY>" />
    
    <!-- Enter keyword, hashtag, or handle here -->
    <add key="twitter_keywords" value="<KEYWORD>" />
    
    <!-- Service Bus specific app setings for messaging connections -->
    <add key="EventHubConnectionString" value="Endpoint=sb://<SERVICEBUS_NAMESPACE>.servicebus.windows.net/;SharedAccessKeyName=<ACCESS_POLICY>;SharedAccessKey=<ACCESS_KEY>" />
    <add key="EventHubName" value="<EVENTHUB_NAME>" />
  </appSettings>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>